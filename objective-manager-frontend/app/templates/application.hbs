<h1>Objective manager</h1>

<table class='objective-table'>
  <thead>
    <th>Title</th>
    <th>Weight</th>
  </thead>

  <tbody>
    {{#each @model as |objective|}}
      <tr>
        <td class='js-objective-title'>{{objective.title}}</td>
        <td class='js-objective-weight'>{{objective.weight}}%</td>
      </tr>
    {{/each}}
  </tbody>
</table>
